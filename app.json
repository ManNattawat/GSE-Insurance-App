{
  "expo": {
    "name": "GSE Enterprise",
    "slug": "GSE-Insurance-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gse.insuranceapp",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "name": "GSE Enterprise",
      "shortName": "GSE",
      "lang": "th",
      "scope": "/",
      "themeColor": "#007AFF",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "description": "แอปพลิเคชันประกันภัยสำหรับ GSE Insurance"
    }
  }
}
